@startuml
!include style.puml
skinparam arrowThickness 1.1
skinparam arrowColor MODEL_COLOR
skinparam classBackgroundColor MODEL_COLOR

Package "Domain Entities" as ModelPackage <<Rectangle>>{

Class Person
Class TuitionClass
Class Address
Class Email
Class Student
Class Tutor
Class Parent
Class PersonId
Class Category
Class Name
Class Phone
Class Tag
Class ClassId
Class Day
Class Time

Class I #FFFFFF
}

Class HiddenOutside #FFFFFF
HiddenOutside --> TuitionClass
HiddenOutside --> Person

Student -up-|> Person
Tutor -up-|> Person
Parent -up-|> Person
Person *--> "  1" PersonId
Person *--> "1" Category
Person *--> "*" Tag
Person *--> "1" Name
Person *--> "1" Phone
Person *--> "1" Email
Person *--> "1" Address

Student --> "0..1" PersonId : linkedParentId
Student --> "0..1" ClassId
Parent --> "*" PersonId : childrenIds

TuitionClass --> "0..1" PersonId : tutorId
TuitionClass --> "*" PersonId : studentIds
TuitionClass --> "1 " ClassId

ClassId --> "1" Day
ClassId --> "1  " Time

Person -[hidden]left-> TuitionClass
Student -[hidden]right-> Parent

Name -[hidden]right-> Phone
Phone -[hidden]right-> Address
Address -[hidden]right-> Email
@enduml
